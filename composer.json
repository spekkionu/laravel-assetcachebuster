{
    "name": "spekkionu/assetcachebuster",
    "description": "Prefixes asset urls with a unique hash which will allow invalidation of asset files cached by the browser.",
    "keywords": ["assets", "cachebuster", "cache", "cdn", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Bernardi",
            "email": "jon@jonbernardi.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-0": {
            "Spekkionu\\Assetcachebuster\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Spekkionu\\Assetcachebuster\\AssetcachebusterServiceProvider"
            ],
            "aliases": {
                "Asset": "Spekkionu\\Assetcachebuster\\Facades\\Cachebuster"
            }
        }
    }
}
